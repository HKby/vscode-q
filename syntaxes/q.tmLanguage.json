{
    "scopeName": "source.q",
    "fileTypes": [
        "q"
    ],
    "patterns": [
        {
            "name": "comment.block.q",
            "begin": "^\/\\s*$",
            "end": "^\\\\\\s*$"
        },
        {
            "name": "comment.block.q",
            "begin": "\\\\\n",
            "end": "\\z$"
        },
        {
            "name": "comment.line.q",
            "match": "\/.*$"
        },
        {
            "name": "string.quoted.double.q",
            "match": "\"(?>[^\\\\\"]++|\\\\.)*+\""
        }
    ]
}